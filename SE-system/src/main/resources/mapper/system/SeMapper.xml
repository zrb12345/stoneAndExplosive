<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.se.system.mapper.SeMapper">

	<resultMap  id="res" type="com.se.system.domain.Se">
		<id     property="id"     column="id"     />
		<result property="averageBlockSize"   column="average_block_size"   />
		<result property="boulderYield"  column="boulder_yield"   />
		<result property="specificCharge"   column="specific_charge"   />
		<result property="overexcavationThickness"   column="overexcavation_thickness"   />
		<result property="type"     column="type"      />
		<result property="status"     column="status"      />
		<result property="delFlag"    column="del_flag"    />
		<result property="createBy"   column="create_by"   />
		<result property="createTime" column="create_time" />
		<result property="updateBy"   column="update_by"   />
		<result property="updateTime" column="update_time" />
	</resultMap>

	<sql id="selectSe">
		select
		i.id,
		i.average_block_size,
		i.boulder_yield,
		i.specific_charge,
		i.overexcavation_thickness,
		i.type,
		i.status,
		i.del_flag,
		i.create_by,
		i.create_time,
		i.update_by,
		i.update_time
		from se i
	</sql>
	<insert id="insertSe">
		insert into se
		(
		<!--		<if test="id != null and id != 0">id,</if>-->
		<if test="averageBlockSize != null ">average_block_size,</if>
		<if test="boulderYield != null ">boulder_yield,</if>
		<if test="specificCharge != null ">specific_charge,</if>
		<if test="overexcavationThickness != null ">overexcavation_thickness,</if>
		<if test="type != null ">type,</if>
		<if test="status != null and status != ''">status,</if>
		<if test="createBy != null and createBy != ''">create_by,</if>
		create_time
		)
		values(
		<!--		<if test="id != null and id != 0">id,</if>-->
		<if test="averageBlockSize != null ">#{averageBlockSize},</if>
		<if test="boulderYield != null ">#{boulderYield},</if>
		<if test="specificCharge != null ">#{specificCharge},</if>
		<if test="overexcavationThickness != null ">#{overexcavationThickness},</if>
		<if test="type != null and type != ''">#{type},</if>
		<if test="status != null and status != ''">#{status},</if>
		<if test="createBy != null and createBy != ''">#{createBy},</if>
		sysdate()
		)
	</insert>
	<insert id="insertList">
		insert into se
		(
		average_block_size, boulder_yield, specific_charge, overexcavation_thickness, type, status, create_by
		)
		values
		<foreach  item="item" index="index" collection="list"  separator=",">
			(
			#{item.averageBlockSize},
			#{item.boulderYield},
			#{item.specificCharge},
			#{item.overexcavationThickness},
			#{item.type},
			#{item.status},
			#{item.createBy}
			)
		</foreach>
	</insert>





	<select id="selectSeList" parameterType="com.se.system.domain.Se" resultMap="res">
		<include refid="selectSe"/>
		where i.del_flag = '0'
		<!--		<if test="deptId != null and deptId != 0">-->
		<!--			AND dept_id = #{deptId}-->
		<!--		</if>-->
		<!--		<if test="parentId != null and parentId != 0">-->
		<!--			AND parent_id = #{parentId}-->
		<!--		</if>-->
		<!--		<if test="deptName != null and deptName != ''">-->
		<!--			AND dept_name like concat('%', #{deptName}, '%')-->
		<!--		</if>-->
		<if test="status != null and status != ''">
			AND status = #{status}
		</if>
		<!-- 数据范围过滤 -->
		${params.dataScope}
		order by i.id
	</select>




</mapper>